{
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "source": {
        "includePattern": ".+\\.js(doc)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "plugins": [],
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false,
        "systemName"            : "tinycmce-plugin-dynamicdate",
        "footer"                : "<center><a href=\"https://github.com/sirap-group\">https://github.com/sirap-group</a></center>",
        "copyright"             : "Copyright SIRAP Group Â© 2016",
        "includeDate"           : true,
        "navType"               : "vertical",
        "theme"                 : "simplex",
        "linenums"              : true,
        "collapseSymbols"       : true,
        "inverseNav"            : false,
        "outputSourceFiles"     : true ,
        "outputSourcePath"      : true,
        "dateFormat"            : "DD/MM/YYYY",
        "syntaxTheme"           : "dark",
        "sort"                  : true
    }
}
